                    <label for="pf_observacion">Observación</label>
                    @Html.TextBoxFor(q => q.gi_Observacion, new { @class = "form-control" })
                    @Html.ValidationMessageFor(q => q.gi_Observacion, "", new { @class = "text-danger" })
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-12">
                <div class="form-group">

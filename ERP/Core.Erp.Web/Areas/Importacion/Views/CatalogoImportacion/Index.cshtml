
@{
    ViewBag.Title = "Consulta de catálogos de importación";
    if (string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.Action("GridViewPartial_catalogo_imp", new { IdCatalogo_tipo = ViewBag.IdCatalogo_tipo })

    <div class="box-footer">
        <a class="btn btn-default" href="@Url.Action("Index","CatalogoTipoImportacion",new { IdCatalogo_tipo = ViewBag.IdCatalogo_tipo, Area = "Importacion" },null)">Volver</a>
    </div>
}